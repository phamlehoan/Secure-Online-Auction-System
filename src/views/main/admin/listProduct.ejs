                <!-- ============================================================== -->
                <div class="row">
                    <!-- Column -->
                    <div class="col-lg-5 col-xlg-4 col-md-6">
                        <div id="lists" class="card">
                            <div class="card-body">
                                <h4 class="card-title">Products/Livestreams</h4>
                            </div>
                            <div class="comment-widgets scrollable">
                                <!-- Comment Row -->
                                <%if(products){%>
                                    <%products.forEach(product => {%>
                                        <div class="d-flex flex-row comment-row m-t-0">
                                            <div class="p-2"><img src="<%-product.image%>" alt="user" width="50"
                                                    class="rounded-circle"></div>
                                            <div class="comment-text w-100">
                                                <h6 class="font-medium" onclick="showProduct('<%-product._id%>')" style="cursor: pointer;"><%-product.name%></h6>
                                                <div class="m-b-15 d-block">
                                                    <span>
                                                        <b><%-product.userId%></b>
                                                        <div class="text-warning d-inline">
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                        </div>
                                                    </span>
                                                    <div>
                                                        <b>Start time:</b>
                                                        <span><%-product.aucStartTime%></span></div>
                                                    <div>
                                                        <b>End time:</b>
                                                        <span><%-product.aucEndTime%></span>
                                                    </div>
                                                    <div>
                                                        <b>Current Price:</b>
                                                        <span><%-product.price%></span>
                                                    </div>
                                                    <div>
                                                        <b>Step:</b>
                                                        <span><%-product.priceStep%></span>
                                                    </div>
                                                    <div>
                                                        <b>Reserve Price:</b>
                                                        <span><%-product.reservePrice%></span>
                                                    </div>      
                                                </div>
                                                <div class="comment-footer">
                                                    <span class="text-muted float-right"></span>
                                                    <%if(product.status == 1){%>
                                                        <span id="<%-product._id%>_stt" class="label label-rounded label-primary">Approved</span>
                                                    <%}else if(product.status == 2){%>
                                                        <span id="<%-product._id%>_stt" class="label label-rounded label-primary">Ended</span>
                                                    <%}else{%>
                                                        <span id="<%-product._id%>_stt" class="label label-rounded label-danger">Rejected</span>
                                                    <%}%>
                                                    <span class="action-icons">
                                                        <%if(product.status == 1){%>  
                                                            <i id="<%-product._id%>_ban" class="ti-na" onclick="return banProduct('<%-product._id%>')" style="cursor: pointer;"></i>
                                                        <%}%>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    <%})%>
                                <%}else{%>
                                    <tr>
                                        <h3>No products in the system.</h3>
                                    </tr>
                                <%}%>
                                <!-- Comment Row -->
                                
                            </div>
                        </div>
                    </div>
                    <!-- Column -->
                    <!-- column -->
                    <div class="col-lg-7 col-xlg-8 col-md-6">
