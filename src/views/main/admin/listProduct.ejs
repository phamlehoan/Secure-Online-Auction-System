                                                    <li data-target="#product_img" data-slide-to="1"></li>
                                                    <li data-target="#product_img" data-slide-to="2"></li>
                                                    <li data-target="#product_img" data-slide-to="3"></li>
                                                </ol>
                                                <div class="carousel-inner" role="listbox">
